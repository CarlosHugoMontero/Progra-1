// ciclos productofines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;
void main()
{
	int i,n,producto;
	producto=1;
	do 
	{
		cout<<"Ingrese el numero de enteros que quiere saber su producto: ";
		cin>>n;
	}while(n<=0 );
		for (i=1;i<=n;i++)
			producto=producto*i;
	cout<<"El resultado del producto sera: ";
	cout<<producto;
	getch();
}
